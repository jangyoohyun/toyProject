function btnUserUpdate(userId) {
	
	let data = $("#userUpdate").serialize();
	
	console.log(data);
	
	$.ajax({
		type: "put",
		url: `/api/auth/${userId}/update`,
		data: JSON.stringify(data),
		contentType: "application/json; charset=UTF-8",
		dataType: "json"
	}).done(res => {
		console.log("성공", res);
		location.href="/";
	}).fail(error =>{
		alert(JSON.stringify(error));
	});
	
}