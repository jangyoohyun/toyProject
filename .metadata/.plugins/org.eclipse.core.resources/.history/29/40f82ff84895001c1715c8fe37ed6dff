package com.study.toyproject.web.dto;

import com.study.toyproject.domain.user.User;

import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
public class UserUpdateDto {
	
	private String name;
	private String phone;
	
	public User toEntity() {
		return User.builder()
				.name(name)
				.phone(phone)
				.build();
	}

}
