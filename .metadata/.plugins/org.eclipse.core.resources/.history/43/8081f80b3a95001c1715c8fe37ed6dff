let photoList = {
	init: function() {
		$("#btn-photoUpdate").on("click", () => {
			this.photoUpdate();
		});

		$("#btn-deleteById").on("click", () => {
			this.deleteById();
		});
	},

	deleteById: function() {

		let id = $("#id").val();

		var deleteConfirm = confirm("글을 삭제하시겠습니까?");

		if (deleteConfirm == true) {
			$.ajax({
				type: "delete",
				url: "/api/photo/photoDelete/" + id,
				dataType: "json"
			}).done(res => {
				location.href = "/photo";
			}).fail(error => {
				console.log(error);
			})
		} else {
			return false;
		}
	}

}

photoList.init();